      <div class="row center" id="giftSection">
        <div class="col s12 m4">
          <div class="icon-block">
            <h2 class="center red-text accent-1"><i class="material-icons">loyalty</i></h2>
            <h5 class="center red-text accent-1">Available Gifts</h5>
            {{#each wedding}}
            {{#unless this.reserved}}

            <div class="well well-lg"><b>{{this.gift_name}}</b>
            <form action="/{{this.id}}/0?_method=PUT" method="POST">
              <button type="submit">Reserve</button>
            </form><form action="/{{this.id}}/0?_method=PUT" method="POST">
            </form></div>
            {{/unless}}
            {{/each}}
          </div>
        </div>


        <div class="col s12 m4">
          <div class="icon-block">
            <h2 class="center red-text accent-1"><i class="material-icons">card_giftcard</i></h2>
            <h5 class="center red-text accent-1">Reserved Gifts</h5>
            {{#each wedding}}
            {{#if this.reserved}}
            <div class="well well-lg"><b>{{this.gift_name}}</b>
            </div>
           {{/if}}
           {{/each}}
          </div>
        </div>
      </div>

    </div>
  </div>


